<p id="notice"><%= notice %></p>

<h1>Sponsors And Supporters</h1>

<table>
  <thead>
    <tr>
      <th>Image</th>
      <th>Link</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sponsors_and_supporters.each do |sponsors_and_supporter| %>
      <tr>
        <td><%= sponsors_and_supporter.image %></td>
        <td><%= sponsors_and_supporter.link %></td>
        <td><%= link_to 'Show', sponsors_and_supporter %></td>
        <td><%= link_to 'Edit', edit_sponsors_and_supporter_path(sponsors_and_supporter) %></td>
        <td><%= link_to 'Destroy', sponsors_and_supporter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sponsors And Supporter', new_sponsors_and_supporter_path %>
