<%= form_for :subscription, url: form_pagamento_path do |f| %>
  <%= f.label :CPF %>
  <%= f.text_field :cpf %>
  
  <%= f.label :Nome %>
  <%= f.text_field :nome %>
  
  <%= f.label :Email %>
  <%= f.text_field :email %>
  
  <%= f.label :Telefone %>
  <%= f.text_field :telefone %>
  
  <%= f.submit %>
<% end %>
